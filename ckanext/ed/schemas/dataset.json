{
  "scheming_version": 1,
  "dataset_type": "dataset",
  "about": "Dataset schema for US Dept. of Education",
  "about_url": "https://www.viderum.com/",
  "dataset_fields": [
    {
      "field_name": "title",
      "label": "Title",
      "preset": "title",
      "required": true,
      "validators": "scheming_required unicode",
      "form_placeholder": "eg. A descriptive title"
    },
    {
      "field_name": "name",
      "label": "URL",
      "preset": "dataset_slug",
      "form_placeholder": "eg. my-dataset"
    },
    {
      "field_name": "notes",
      "label": "Description",
      "required": true,
      "form_snippet": "markdown.html",
      "form_placeholder": "eg. Some useful information about the data"
    },
    {
      "field_name": "owner_org",
      "label": "Organization",
      "preset": "dataset_organization"
    },
    {
      "field_name": "keyword",
      "label": "Tags",
      "preset": "tag_string_autocomplete",
      "required": true,
      "validators": "ignore_missing",
      "form_placeholder": "eg. university, public school etc.",
      "help_text": "Enter at least three keywords"
    },
    {
      "field_name": "modified",
      "label": "Last Update",
      "help_text": "Last updated date",
      "required": true,
      "form_snippet": "date.html",
      "validators": "ignore_missing unicode isodate"
    },
    {
      "field_name": "publisher",
      "required": true,
      "label": "Publisher"
    },
    {
      "field_name": "contactPoint",
      "required": true,
      "label": "Contact Name"
    },
    {
      "field_name": "mbox",
      "required": true,
      "label": "Contact Email"
    },
    {
      "field_name": "identifier",
      "required": true,
      "label": "Unique Identifier"
    },
    {
      "field_name": "accessLevel",
      "label": "Public Access Level",
      "preset": "select",
      "required": true,
      "choices": [
        {
          "value": "public",
          "label": "Public"
        },
        {
          "value": "restricted-public",
          "label": "Restricted public"
        },
        {
          "value": "non-public",
          "label": "Non-public"
        }
      ]
    },
    {
      "field_name": "maintainer",
      "label": "Maintainer",
      "form_placeholder": "Joe Bloggs",
      "required": true,

      "display_property": "dc:contributor"
    },
    {
      "field_name": "maintainer_email",
      "label": "Maintainer Email",
      "form_placeholder": "joe@example.com",
      "display_property": "dc:contributor",
      "display_snippet": "email.html",
      "required": true,
      "display_email_name_field": "maintainer"
    }
  ],
  "resource_fields": [
    {
      "field_name": "url",
      "label": "URL",
      "required": true,
      "preset": "resource_url_upload"
    },
    {
      "field_name": "name",
      "label": "Name",
      "required": true,
      "form_placeholder": "eg. January 2011 Gold Prices"
    },
    {
      "field_name": "description",
      "label": "Description",
      "required": true,
      "form_snippet": "markdown.html",
      "form_placeholder": "Some useful notes about the data"
    },
    {
      "field_name": "format",
      "label": "Format",
      "preset": "resource_format_autocomplete"
    }
  ]
}
